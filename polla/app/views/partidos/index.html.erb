<h1>Listing partidos</h1>

<table>
  <thead>
    <tr>
      <th>Equipo1</th>
      <th>Equipo2</th>
      <th>Res1</th>
      <th>Res2</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @partidos.each do |partido| %>
      <tr>
        <td><%= partido.equipo1 %></td>
        <td><%= partido.equipo2 %></td>
        <td><%= partido.res1 %></td>
        <td><%= partido.res2 %></td>
        <td><%= link_to 'Show', partido %></td>
        <td><%= link_to 'Edit', edit_partido_path(partido) %></td>
        <td><%= link_to 'Destroy', partido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partido', new_partido_path %>
